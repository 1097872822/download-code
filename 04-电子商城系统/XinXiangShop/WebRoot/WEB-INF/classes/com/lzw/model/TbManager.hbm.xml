<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.lzw.model.TbManager" table="tb_manager" schema="dbo" catalog="db_shop">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="assigned" />
        </id>
        <property name="manager" type="java.lang.String">
            <column name="manager" length="30" not-null="true" />
        </property>
        <property name="pwd" type="java.lang.String">
            <column name="PWD" length="30" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
