<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.lzw.model.TbRebate" table="tb_rebate" schema="dbo" catalog="db_shop">
        <id name="grade" type="java.lang.String">
            <column name="grade" length="20" />
            <generator class="assigned" />
        </id>
        <property name="amount" type="java.lang.Double">
            <column name="Amount" scale="4" not-null="true" />
        </property>
        <property name="rebate" type="java.lang.Float">
            <column name="rebate" precision="53" scale="0" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
